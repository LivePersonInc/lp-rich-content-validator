{
  "title": "gbmDescription",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["type", "text"],
  "properties": {
    "type": {
      "type": "string",
      "enum": ["text"],
      "default": "text",
      "readonly": true
    },
    "tag": {
      "type": "string",
      "enum": ["subtitle"]
    },
    "text": {
      "type": "string",
      "maxLength": 2000
    },
    "tooltip": {
      "type": "string"
    }
  }
}
