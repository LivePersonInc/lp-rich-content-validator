{
  "title": "viberButton",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["click", "title", "type"],
  "properties": {
    "tooltip": {
      "type": "string",
      "maxLength": 256
    },
    "title": {
      "type": "string",
      "maxLength": 128
    },
    "type": {
      "type": "string",
      "enum": ["button"],
      "default": "button",
      "readonly": true
    },
    "click": { "$ref": "viberClickOperations.json" }
  }
}
