{
  "title": "viberSubtitle",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["type", "text"],
  "properties": {
    "type": {
      "type": "string",
      "enum": ["text"],
      "default": "text",
      "readonly": true
    },
    "tag": {
      "type": "string",
      "enum": ["subtitle"],
      "default": "subtitle",
      "readonly": true
    },
    "text": {
      "type": "string",
      "maxLength": 640
    },
    "tooltip": {
      "type": "string",
      "maxLength": 256
    },
    "style": { "$ref": "viberElementStyle.json" }
  }
}
